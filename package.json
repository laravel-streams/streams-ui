        "@types/lz-string": "^1.3.34",
        "@types/mocha": "^9.0.0",
        "@types/mousetrap": "^1.6.9",
        "@types/node": "^16.10.1",
        "@types/should": "^13.0.0",
        "@types/should-sinon": "^0.0.8",
