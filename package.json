{
    "name": "@laravel-streams/ui",
    "version": "1.0.0",
    "types": "./resources/public/types/index.d.ts",
    "main": "./resources/public/js/index.js",
    "scripts": {
        "dev": "mix",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "dts": "tsc --project webpack.tsconfig.json --emitDeclarationOnly",
        "dts:copy": "cp resources/lib/global.d.ts resources/public/js/global.d.ts",
        "test": "TS_NODE_PROJECT=./tsconfig.test.json mocha"
    },
    "streams": {
        "output": {
            "name": [
                "streams",
                "ui"
            ],
            "type": "assign"
        },
        "src": "resources/lib",
        "bundler": "mix",
        "scripts": {
            "dev": "dev",
            "prod": "build",
            "watch": "watch",
            "test": "test"
        }
    },
    "dependencies": {
