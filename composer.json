{
    "name": "streams/ui",
    "description": "The official UI package for Laravel Streams.",
    "type": "streams-addon",
    "authors": [
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com"
        }
    ],
    "require": {
        "streams/core": "^2.0",
        "blade-ui-kit/blade-icons": "^1.1",
        "blade-ui-kit/blade-heroicons": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19"
    },
    "autoload": {
        "psr-4": {
            "Streams\\Ui\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Streams\\Ui\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Streams\\Ui\\UiServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
